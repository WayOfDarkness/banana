{% block pre_include %}
    <title>The Autumn Night</title>
    <link rel="stylesheet" href="{{theme.uri}}/css/riddle.css">
    <link rel="shortcut icon" href="{{settings.favicon | upload_url}}">
{% endblock %}

{% block post_include %}{% endblock %}

{% block content %}
    
    <img class="autumn" src="{{theme.uri}}/img/four-seasons/riddles/autumn/autumn_1.jpg" alt="The Autumn Night" usemap="#planetmap">
    <map name="planetmap">
        <area coords="670,155,90" shape="circle" title="October 4, 2009">
        <area coords="1000,68,3" shape="circle" title="November 1, 2001">
        <area coords="327,270,3" shape="circle" title="February 16, 2003">
        <area coords="1066,252,3" shape="circle" title="July 31, 2004">
        <area coords="1466,305,2" shape="circle" title="May 9, 2009">
        <area coords="750,550,936,345,1510,335,1510,706,750,706" shape="poly" title="House?">
    </map>

{% endblock content %}