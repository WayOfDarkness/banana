{% block pre_include %}
    <title>The Autumn Night</title>
    <link rel="stylesheet" href="{{theme.uri}}/css/riddle.css">
    <link rel="shortcut icon" href="{{settings.favicon | upload_url}}">
{% endblock %}

{% block post_include %}{% endblock %}

{% block content %}
    
    <img class="autumn" src="{{theme.uri}}/img/four-seasons/riddles/autumn/autumn_1.jpg" alt="The Autumn Night" usemap="#planetmap">
    <map name="planetmap">
        <area coords="670,155,90" shape="circle" title="October 4, 2009">
        <area coords="990,68,3" shape="circle" title="November 1, 2001">
        <area coords="325,273,3" shape="circle" title="February 16, 2003">
        <area coords="1060,255,3" shape="circle" title="July 31, 2004">
        <area coords="1460,308,2" shape="circle" title="May 9, 2009">
        <area coords="750,550,930,345,1500,335,1500,700,750,700" shape="poly" title="House?">
    </map>

{% endblock content %}