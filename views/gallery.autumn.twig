{% block pre_include %}
    <title>The Autumn Night</title>
    <link rel="stylesheet" href="{{theme.uri}}/css/riddle.css">
    <link rel="shortcut icon" href="{{settings.favicon | upload_url}}">
{% endblock %}

{% block post_include %}
{% endblock %}

{% block content %}

    <img class="autumn" src="{{theme.uri}}/img/four-seasons/riddles/autumn/autumn_1.jpg" alt="The Autumn Night" usemap="#planetmap">
    <map name="planetmap">
        <area coords="680,160,90" shape="circle" title="October 4, 2009">
        <area coords="1010,68,3" shape="circle" title="November 1, 2001">
        <area coords="332,275,3" shape="circle" title="February 16, 2003">
        <area coords="1082,257,3" shape="circle" title="July 31, 2004">
        <area coords="1485,310,2" shape="circle" title="May 9, 2009">
        <area coords="760,560,948,355,1530,345,1530,716,760,716" shape="poly" title="House?">
    </map>

{% endblock content %}